<!-- Modal -->
<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" style="font-family: kg_second_chances_sketchRg;">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h2>Meet Our Flavors</h2>
      </div>
      <div class="modal-body">
  </div><!--row-->
  <div class="row">
    <div class="col-md-3">
    <img src="https://s3.amazonaws.com/plonkers/public/flavor-cheese.png">
    </div><!--col-->    
    <div class="col-md-3 col-md-offset-1">
    <img src="https://s3.amazonaws.com/plonkers/public/flavor-caramel.png">
    </div><!--col-->
    <div class="col-md-3 col-md-offset-1">
    <img src="https://s3.amazonaws.com/plonkers/public/flavor-apple.png">
    </div><!--col--> 
  </div><!--row-->
  <div class="row">
    <div class="col-md-4 col-md-offset-1">
    <img src="https://s3.amazonaws.com/plonkers/public/flavor-kettle.png">
    </div><!--col-->
    <div class="col-md-5 col-md-offset-1">
    <img src="https://s3.amazonaws.com/plonkers/public/flavor-butter.png">
    </div><!--col-->
  </div><!--row-->
      <div class="modal-footer">
        <center><h4>Interested in buying Plonkers Popcorn? </h4>
        <button type="button" class="btn btn-primary"><%= link_to "Contact Us", contact_path %> </button></center>
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>